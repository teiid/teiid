<?xml version="1.0" encoding="UTF-8"?>
<module xmlns="urn:jboss:module:1.3" name="org.apache.olingo.server">
    <properties>
        <property name="jboss.api" value="private"/>
    </properties>
    
    <resources>
        <artifact name="${org.jboss.teiid:teiid-olingo-patches}"/>
        <artifact name="${org.apache.olingo:odata-server-api}"/>
        <artifact name="${org.apache.olingo:odata-server-core}"/>
        <artifact name="${org.apache.olingo:odata-server-core-ext}"/>        
