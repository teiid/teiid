SET NAMESPACE 'http://www.teiid.org/translator/couchbase/2017' AS teiid_couchbase;

CREATE FOREIGN TABLE test (
	attr_double double,
	attr_number_short short,
	attr_string string,
	attr_boolean boolean,
	attr_number_long long,
	attr_int integer,
	Name string,
	attr_number_integer integer,
	attr_long long,
	attr_number_float float,
	attr_null object,
	attr_number_byte byte,
	attr_number_double double
) OPTIONS (NAMEINSOURCE '`test`', UPDATABLE TRUE, "teiid_couchbase:ISARRAYTABLE" 'false', "teiid_couchbase:ISTOPTABLE" 'true');

CREATE FOREIGN TABLE test_attr_jsonArray (
	attr_jsonArray object
) OPTIONS (NAMEINSOURCE '`test`.`attr_jsonArray`', UPDATABLE TRUE, "teiid_couchbase:ISARRAYTABLE" 'true', "teiid_couchbase:ISTOPTABLE" 'false');

CREATE FOREIGN TABLE test_attr_jsonObject (
	attr_double double,
	attr_number_short short,
	attr_string string,
	attr_boolean boolean,
	attr_number_long long,
	attr_int integer,
	Name string,
	attr_number_integer integer,
	attr_long long,
	attr_number_float float,
	attr_null object,
	attr_number_byte byte,
	attr_number_double double
) OPTIONS (NAMEINSOURCE '`test`.`attr_jsonObject`', UPDATABLE TRUE, "teiid_couchbase:ISARRAYTABLE" 'false', "teiid_couchbase:ISTOPTABLE" 'false');