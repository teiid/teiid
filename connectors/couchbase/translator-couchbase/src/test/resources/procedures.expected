CREATE FOREIGN PROCEDURE deleteDocument(IN id string OPTIONS (ANNOTATION 'The document id or SQL like pattern of what documents to delete, for example, the ''%'' sign is used to define wildcards (missing letters) both before and after the pattern')) RETURNS TABLE (result clob)
OPTIONS (ANNOTATION 'Delete the docuemnt via id');

CREATE FOREIGN PROCEDURE getDocument(IN id string OPTIONS (ANNOTATION 'The document id of what documents to return')) RETURNS TABLE (result blob)
OPTIONS (ANNOTATION 'Return a json document that match the given document id as BLOB');

CREATE FOREIGN PROCEDURE getDocuments(IN id string OPTIONS (ANNOTATION 'The document id or SQL like pattern of what documents to return, for example, the ''%'' sign is used to define wildcards (missing letters) both before and after the pattern')) RETURNS TABLE (result blob)
OPTIONS (ANNOTATION 'Returns json documents that match the given document id or id pattern as BLOBs');

CREATE FOREIGN PROCEDURE getMetadataDocument() RETURNS TABLE (result blob)
OPTIONS (ANNOTATION 'Get metadata json document as BLOB');

CREATE FOREIGN PROCEDURE getTextDocument(IN id string OPTIONS (ANNOTATION 'The document id of what document to return')) RETURNS TABLE (id string, result clob)
OPTIONS (ANNOTATION 'Return a json document that match the given document id as CLOB');

CREATE FOREIGN PROCEDURE getTextDocuments(IN id string OPTIONS (ANNOTATION 'The document id or SQL like pattern of what documents to return, for example, the ''%'' sign is used to define wildcards (missing letters) both before and after the pattern')) RETURNS TABLE (id string, result clob)
OPTIONS (ANNOTATION 'Returns json documents that match the given document id or id pattern as CLOBs');

CREATE FOREIGN PROCEDURE getTextMetadataDocument() RETURNS TABLE (result clob)
OPTIONS (ANNOTATION 'Get metadata json document as CLOB');

CREATE FOREIGN PROCEDURE saveDocument(IN id string OPTIONS (ANNOTATION 'The document id used to save the document'), IN document object OPTIONS (ANNOTATION 'The contents to save.  Can be one of CLOB, BLOB, or json String')) RETURNS TABLE (result clob)
OPTIONS (ANNOTATION 'Saves the given value with the given id.  Any existing document will be overriden.');