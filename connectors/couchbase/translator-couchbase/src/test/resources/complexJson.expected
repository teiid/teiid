CREATE FOREIGN TABLE A (
	id string OPTIONS (UPDATABLE FALSE),
	CONSTRAINT PK0 PRIMARY KEY(id)
) OPTIONS (NAMEINSOURCE '`A`', UPDATABLE TRUE);

CREATE FOREIGN TABLE A_A (
	id string OPTIONS (UPDATABLE FALSE)
) OPTIONS (NAMEINSOURCE '`A`.`A`', UPDATABLE TRUE);

CREATE FOREIGN TABLE A_A_A (
	id string OPTIONS (UPDATABLE FALSE),
	A string
) OPTIONS (NAMEINSOURCE '`A`.`A`.`A`', UPDATABLE TRUE);