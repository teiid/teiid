<?xml version="1.0" encoding="UTF-8"?>
<module xmlns="urn:jboss:module:1.3" name="org.arrahtec">
	<properties>
		<property name="jboss.api" value="public" />
	</properties>
	<resources>
		<resource-root path="itextpdf-${version.com.itextpdf}.jar" />
		<resource-root path="java-string-similarity-${version.info.debatty}.jar" />
		<resource-root path="jxl-${version.net.sourceforge.jexcelapi}.jar" />
		<resource-root path="opencsv-${version.com.opencsv}.jar" />
		<resource-root path="osdq-core-${version.org.arrahtec}.jar" />
		<resource-root path="simmetrics-core-${version.com.github.mpkorstanje}.jar" />
		
		<artifact name="${com.itextpdf:itextpdf}"/>
		<artifact name="${org.arrahtec:osdq-core}"/>
		<artifact name="${com.github.mpkorstanje:simmetrics-core}"/>
		<artifact name="${com.opencsv:opencsv}"/>
		<artifact name="${info.debatty:java-string-similarity}"/>
		<artifact name="${net.sourceforge.jexcelapi:jxl}"/>
		<artifact name="${net.sourceforge.jeval:jeval}"/>
	</resources>

	<dependencies>
		<module name="javax.api" />
		<module name="javax.activation.api" />
		<module name="org.apache.log4j" />
		<module name="org.slf4j" />
		<module name="org.apache.lucene" />
		<module name="com.google.guava" />
	</dependencies>
</module>


